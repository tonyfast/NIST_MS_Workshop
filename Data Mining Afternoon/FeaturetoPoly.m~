% function P = FeaturetoPoly( data, pord );

nd = size( data, 2);

permid = mat2cell( 1 : nd, 1, ones(1,nd) );
inds = [ repmat([1:nd]',1,pord); perms( 1 : pord )];
ct = nd;
for ii = 2 : pord
    u = unique( inds(:,1:ii),'rows');
    for nn = 1 : size(u,1)
        ct = ct + 1;
        permid{ct} = u(nn,:);
    end
end

for ii = 1 : numel( permid )
    P(:,ii) = data(:,permid{ii}(
    for nn = 2 : numel( permid{ii} )
    end
end